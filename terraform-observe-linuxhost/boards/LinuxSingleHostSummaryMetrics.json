                    "legend": {
                      "placement": "right",
                      "type": "list",
                      "visible": false
                    },
                    "type": "xy",
                    "xConfig": {
                      "visible": false
                    },
                    "yConfig": {
                      "unit": "%",
                      "visible": true
                    }
                  },
                  "source": {
                    "metric": {
                      "aggregate": "sum",
                      "datasetId": "$dataset_telegraf_memMetrics",
                      "description": "Percentage of total memory in use\n",
                      "groupBy": [
                        "host",
                        "datacenter"
                      ],
                      "heuristics": {
                        "__typename": "MetricHeuristics",
                        "cardinality": "2",
                        "numOfPoints": "720",
                        "validLinkLabels": [
                          "Host"
                        ]
                      },
                      "id": {
                        "datasetId": "$dataset_telegraf_memMetrics",
                        "name": "used_percent"
