                        "label": "Host",
                        "srcFields": [
                          "host",
                          "datacenter"
                        ],
                        "targetDataset": "$dataset_server_host",
                        "targetStageLabel": null,
                        "type": "foreign"
                      },
                      "name": "used_percent",
                      "rollup": "avg",
                      "type": "gauge",
                      "unit": "%",
                      "userDefined": true
                    },
                    "type": "metric"
                  },
                  "type": "timeseries"
                }
